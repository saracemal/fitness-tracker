<h1> Your Entries </h1> 


<% @entries.each do |entry| %> 
    <% if entry.user_id == cookies[:user_id].to_i %>
        <ul>
            <%= entry.routine.name%>
            <% if entry.%>
        </ul>
        
        


        <li><%= link_to entry.title, entry_path(entry) %></li>
    <% end %>
<% end %>







