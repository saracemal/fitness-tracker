<div class="container-fluid">
    <div class="container">
        <h1> Your Entries </h1> 

        <% @entries.each do |entry| %> 
            <% if entry.user_id == @current_user.id %>
                <ul class="list-group">
                    <%= entry.routine.name%>
                    <li class="list-group-item"><%= link_to entry.title, entry_path(entry) %></li><br>
                </ul>
            <% end %>
        <% end %>
    </div>
